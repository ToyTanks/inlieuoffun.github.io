---
---
{
  "guests": [
    {% for guest in site.data.guests -%}
    {
      "name":{{ guest.name | jsonify }},
      "episodes":{{ guest.episodes | jsonify }}
      {%- if guest.twitter %},
      "twitter":{{ guest.twitter | jsonify }}{% endif %}
      {%- if guest.url %},
      "url":{{ guest.url | jsonify }}{% endif %}
    }{% unless forloop.last %},	
    {% endunless %}
    {%- endfor %}
  ]
}
